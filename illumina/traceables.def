// Define here values that can be traced for each node in our search tracer.
// The only supported types for traceables are std::string, Move, i64 and bool.
// Define a new traceable value by calling TRACEABLE(...args).

#ifndef TRACEABLE
#define TRACEABLE(name, cpp_type)
#endif

TRACEABLE(QSEARCH,     bool)
TRACEABLE(PV,          bool)
TRACEABLE(ALPHA,       i64)
TRACEABLE(BETA,        i64)
TRACEABLE(STATIC_EVAL, i64)
TRACEABLE(SCORE,       i64)
TRACEABLE(DEPTH,       i64)
TRACEABLE(ZOB_KEY,     i64)
TRACEABLE(SKIP_MOVE,   Move)
TRACEABLE(BEST_MOVE,   Move)
TRACEABLE(LAST_MOVE,   Move)
TRACEABLE(TT_CUTOFF,   bool)
TRACEABLE(FOUND_IN_TT, bool)
TRACEABLE(IMPROVING,   bool)
TRACEABLE(FEN,  std::string)

#undef TRACEABLE
